@model RegistrationModel

<div class="form-group">
    @Html.LabelFor(model => model.RegisterData.Email, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.EditorFor(model => model.RegisterData.Email, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.RegisterData.Email, "", new { @class = "text-danger" })
    </div>
</div>

<div class="form-group">
    @Html.LabelFor(model => model.RegisterData.Password, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.PasswordFor(model => model.RegisterData.Password, new { @class = "form-control" })
        @Html.ValidationMessageFor(model => model.RegisterData.Password, "", new { @class = "text-danger" })
    </div>
</div>

<div class="form-group">
    @Html.LabelFor(model => model.RegisterData.ConfirmPassword, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.PasswordFor(model => model.RegisterData.ConfirmPassword, new { @class = "form-control" })
        @Html.ValidationMessageFor(model => model.RegisterData.ConfirmPassword, "", new { @class = "text-danger" })
    </div>
</div>
